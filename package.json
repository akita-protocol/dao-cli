{
  "name": "@akta/dao-cli",
  "version": "0.1.1",
  "description": "Read-only CLI and TUI for querying Akita DAO state on Algorand",
  "type": "module",
  "bin": {
    "akita-dao": "./src/index.ts"
  },
  "scripts": {
    "start": "bun run src/index.ts",
