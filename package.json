  },
  "dependencies": {
    "@akta/sdk": "0.0.1",
    "@algorandfoundation/algokit-utils": "9.1.2",
    "algosdk": "3.5.2",
    "commander": "^13.1.0",
    "chalk": "^5.4.1"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "sharp": "^0.34.5",
